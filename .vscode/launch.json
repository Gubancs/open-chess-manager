    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build - command-line-tool",
            "program": "${workspaceFolder}/command-line-tool/bin/cli.js",
            "args": ["rtg", "./output/tournament.trf", "-p 9", "-r 3"],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}