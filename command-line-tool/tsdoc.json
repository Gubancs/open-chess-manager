{
    "tags": {
        "allowUnknownTags": true
    },
    "plugins": [
        "plugins/markdown",
        "/Users/kokeny/dev/git/open-chess-manager/command-line-tool/node_modules/tsdoc/template/plugins/TSDoc.js"
    ],
    "opts": {
        "template": "/Users/kokeny/dev/git/open-chess-manager/command-line-tool/node_modules/tsdoc/template",
        "recurse": "true"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "source": {
        "includePattern": "(\\.d)?\\.ts$"
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "tsdoc": {
        "source": "./src",
        "destination": "./docs",
        "tutorials": "",
        "systemName": "command-line-tool",
        "footer": "",
        "copyright": "command-line-tool Copyright Â© 2021 Gabor Kokeny.",
        "outputSourceFiles": true,
        "commentsOnly": true
    }
}
